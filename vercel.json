{
  "version": 2,
  "builds": [
    {
      "src": "nuxt.config.ts",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "serverFiles": [
          "server/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "functions": {
    "server/api/chat.post.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_VERSION": "22"
  }
}